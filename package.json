{
  "name": "rabscuttle",
  "version": "1.0.0",
  "description": "A discord bot written in typescript",
  "main": "src/index.ts",
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/formdata": "^0.10.0",
    "@types/node": "^18.7.23",
    "@types/node-fetch": "^2.5.10",
    "@types/ws": "^7.4.4",
    "gts": "^3.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "nodemonConfig": {
    "exec": "ts-node src/robot/index.ts",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "scripts": {
    "develop": "nodemon",
    "build": "tsc",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord-api-types": "^0.37.11",
    "form-data": "^3.0.1",
    "jimp": "^0.16.1",
    "jsmegahal": "^1.2.4",
    "node-fetch": "^2.6.7",
    "wikipedia": "^1.1.9",
    "winston": "^3.3.3",
    "ws": "^8.9.0",
    "zlib-sync": "^0.1.7"
  }
}
