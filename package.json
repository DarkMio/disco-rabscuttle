{
  "name": "rabscuttle",
  "version": "1.0.0",
  "description": "A discord bot written in typescript",
  "main": "src/index.ts",
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/formdata": "^0.10.0",
    "@types/jimp": "^0.2.28",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.10",
    "@types/ws": "^7.4.4",
    "gts": "^3.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.0"
  },
  "nodemonConfig": {
    "exec": "ts-node src/index.ts",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "scripts": {
    "develop": "nodemon",
    "build": "tsc",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord-api-types": "^0.18.1",
    "discord.js": "git+https://github.com/discordjs/discord.js.git#e300518597955abf4bf3c3d2634b47b9b3964274",
    "form-data": "^3.0.1",
    "jimp": "^0.16.1",
    "jsmegahal": "^1.2.4",
    "node-fetch": "^2.6.1",
    "winston": "^3.3.3"
  }
}
